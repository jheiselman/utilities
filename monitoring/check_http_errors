#!/bin/bash

SCRIPT_PATH=$(dirname $(realpath $BASH_SOURCE))

. "$SCRIPT_PATH/common.sh"

threshold=10

num_errors=$($HOME/scripts/http_errors -c)
if [[ $num_errors -gt $threshold ]]; then
  echo "BAD;More than $threshold errors in the last five minutes"
else
  echo "OK;Number of errors below threshold ($threshold)"
fi
